    "jest": "^29.2.1",
    "jsc-android": "^250231.0.0",
    "lint-staged": "^13.2.0",
    "metro-react-native-babel-preset": "0.76.1",
    "metro-runtime": "^0.76.2",
    "pod-install": "^0.1.38",
    "react-native-flipper": "^0.189.0",
    "react-native-gradle-plugin": "^0.71.17",
    "react-native-mmkv-flipper-plugin": "^1.0.0",
    "react-query-native-devtools": "^4.0.0",
    "react-test-renderer": "18.2.0",
    "tailwindcss": "^3.3.1",
    "typescript": "4.9.5"
  },
  "jest": {
    "preset": "react-native"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-git",
      "useEmoji": true
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix"
    ]
  },
  "rnx-kit": {
    "kitType": "app",
    "bundle": {
      "treeShake": true
    },
    "alignDeps": {
      "requirements": [
        "react-native@0.71"
      ],
      "capabilities": [
        "animation",
        "babel-preset-react-native",
        "community/cli-android",
        "community/cli-ios",
        "core",
        "core-android",
        "core-ios",
        "datetime-picker",
        "gestures",
        "jest",
        "navigation/native",
        "navigation/stack",
        "react",
        "react-test-renderer",
        "safe-area",
        "screens"
      ]
